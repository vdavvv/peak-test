generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StockPrice {
  id        Int      @id @default(autoincrement())
  symbol    String
  price     Float
  createdAt DateTime @default(now())

  @@index([symbol, createdAt])
}
